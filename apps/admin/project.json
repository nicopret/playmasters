{
  "name": "admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/admin/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "admin:build",
        "port": 3001,
        "dev": true
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "root": "apps/admin",
        "outputPath": "dist/apps/admin"
      }
    },
    "serve-static": {
      "executor": "@nx/next:serve-static",
      "options": {
        "buildTarget": "admin:build",
        "port": 3001
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec eslint apps/admin/src apps/admin/components apps/admin/lib apps/admin/*.ts --ignore-pattern apps/admin/next-env.d.ts --max-warnings=0"
      }
    }
  }
}
