{
  "name": "mobile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mobile",
  "targets": {
    "sync": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm cap sync",
        "cwd": "apps/mobile"
      }
    },
    "ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm cap open ios",
        "cwd": "apps/mobile"
      }
    },
    "android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm cap open android",
        "cwd": "apps/mobile"
      }
    }
  },
  "tags": []
}
