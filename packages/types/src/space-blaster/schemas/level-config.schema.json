{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://playmasters.dev/schemas/space-blaster/level-config.schema.json",
    "title": "SpaceBlasterLevelConfig",
    "type": "object",
    "required": [ "layoutId" ],
    "properties": {
        "boss": {
            "default": false,
            "type": "boolean"
        },
        "dive": {
            "maximum": 100,
            "minimum": 1,
            "type": "number"
        },
        "enemyTypes": {
            "type": "array"
        },
        "layoutId": {
            "type": "string"
        },
        "scoreMultiplier": {
            "maximum": 100,
            "minimum": 1,
            "type": "number"
        },
        "shooting": {
            "maximum": 100,
            "minimum": 1,
            "type": "number"
        },
        "speed": {
            "maximum": 100,
            "minimum": 1,
            "type": "number"
        },
        "waves": {
            "type": "array"
        }
    }
}
