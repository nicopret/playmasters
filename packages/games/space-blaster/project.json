{
  "name": "space-blaster",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/games/space-blaster/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/games/space-blaster",
        "env": {
          "TS_NODE_COMPILER_OPTIONS": "{\"moduleResolution\":\"node10\",\"module\":\"commonjs\",\"customConditions\":null}"
        },
        "command": "jest --passWithNoTests"
      },
      "inputs": [
        "default",
        "^production",
        "{workspaceRoot}/jest.preset.js",
        {
          "externalDependencies": ["jest"]
        }
      ],
      "outputs": ["{workspaceRoot}/coverage/packages/games/space-blaster"]
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/games/space-blaster",
        "main": "packages/games/space-blaster/src/index.ts",
        "tsConfig": "packages/games/space-blaster/tsconfig.lib.json",
        "assets": ["packages/games/space-blaster/*.md"]
      }
    }
  }
}
